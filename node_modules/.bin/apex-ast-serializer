#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir//bin/sh" ]; then
  "$basedir//bin/sh"  "$basedir/../prettier-plugin-apex/vendor/apex-ast-serializer/bin/apex-ast-serializer" "$@"
  ret=$?
else 
  /bin/sh  "$basedir/../prettier-plugin-apex/vendor/apex-ast-serializer/bin/apex-ast-serializer" "$@"
  ret=$?
fi
exit $ret
